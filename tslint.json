{
  "defaultSeverity": "none",
  "jsRules": {
    "no-empty": true
  },
  "rules": {
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/**/*.tsx",
      "src/**/*.ts"
    ]
  }
}
